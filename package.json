  "engine": {
    "node": ">=14.17.0",
    "npm": ">=8"
  },
  "scripts": {
    "postinstall": "npm run generate-json-es-modules",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand --detectOpenHandles --forceExit",
    "build": "npm run generate-json-es-modules && npm run generate-dist && npm run generate-packages && npm run generate-doc",
    "lint": "eslint src",
    "generate-json-es-modules": "rm -rf src/common/generated && node generateEsModulesFromJson.cjs",
    "generate-dist": "rm -rf dist && mkdir dist && mkdir dist/esm && cp -r src/. dist/esm/ && mkdir dist/cjs && ./node_modules/.bin/babel src --out-dir dist/cjs --copy-files",
    "generate-packages": "node generatePackages.cjs",
    "generate-doc": "npm run generate-lib-doc && npm run generate-cli-doc",
    "generate-lib-doc": "./node_modules/typedoc/bin/typedoc --tsconfig tsconfig.doc.json --disableSources",
    "generate-cli-doc": "node generateCliDoc.cjs"
  },
  "files": [
    "src/",
    "dist/",
    "generateEsModulesFromJson.cjs",
    "iexec_sdk_logo.jpg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iExecBlockchainComputing/iexec-sdk.git"
